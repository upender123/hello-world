pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                phase("clean") {
                  sh "mvn clean"
                  echo 'cleaning completed..'
                }
                phase("compile") {
                  sh "mvn compile"
                  echo 'compile completed..'
                }
            }
        }
        stage('Test') {
            steps {
                phase("test") {
                  sh "mvn test"
                  echo 'test completed..'
                }
                 phase("package") {
                  sh "mvn ackage"
                  echo 'package completed..'
                }     
                 phase("install") {
                  sh "mvn install"
                  echo 'installation completed..'
                }     
            }
        }
        stage('Deploy') {
            steps {
                phase("deploy") {
                 sh "mvn deploy"
                 echo 'Deploying....'
            }
        }
    }
}
